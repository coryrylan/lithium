{
  "name": "lithium",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "npm run lib:sass && npm-run-all --parallel lib:sass:watch docs:build:watch",
    "build": "npm run clean && npm run lib:build && npm run lib:api && npm run docs:build:prod && npm run test:bundle",
    "test:bundle": "bundlesize --config bundlesize.json",
    "all:format": "prettier --list-different './projects/**/*.{ts,scss,html}' --write",
    "clean": "rimraf ./dist",
    "lib:build": "npm run all:format && npm run lib:lint && npm run lib:sass && npm run lib:test && ng build lithium && node modules.js",
    "lib:test": "ng test lithium --watch=false",
    "lib:test:watch": "ng test lithium --watch --code-coverage",
    "lib:sass": "sass-render --q --suffix '.css.ts' -t ./projects/lithium/sass-template.js ./projects/lithium/**/*element.scss",
    "lib:sass:watch": "sass-render --q --suffix '.css.ts' -t ./projects/lithium/sass-template.js ./projects/lithium/**/*element.scss -w",
    "lib:api": "typedoc --options ./typedoc.js && node api-trim.js",
    "lib:lint": "tslint -p . -c ./projects/lithium/tslint.json \"projects/lithium/**/*.ts\" && tslint -p . -c ./projects/docs/tslint.json \"projects/docs/**/*.ts\"",
    "lib:lint:fix": "tslint --fix -p . -c ./projects/lithium/tslint.json \"projects/lithium/**/*.ts\" && tslint --fix -p . -c ./projects/docs/tslint.json \"projects/docs/**/*.ts\"",
    "lib:start": "npm run lib:sass && npm-run-all --parallel lib:sass:watch lib:build:watch",
    "lib:build:watch": "ng build lithium --watch",
    "docs:build:watch": "ng serve docs",
    "docs:watch:prod": "ng serve docs --prod",
    "docs:build": "ng build docs",
    "docs:build:prod": "ng build docs --prod --stats-json --source-map --ts-config projects/docs/tsconfig.prod.json",
    "docs:build:watch:prod": "npm run lib:build && ng serve docs --prod",
    "docs:lint": "ng lint docs",
    "docs:e2e": "ng e2e docs",
    "docs:deploy": "npm run build && firebase deploy",
    "bundle-sourcemaps": "source-map-explorer dist/docs/*.js",
    "bundle-webpack": "webpack-bundle-analyzer dist/docs/stats-es2015.json",
    "api:vscode": "wca analyze 'projects/lithium/**/*.{js,ts}' --outFile=lithium.html-data.json --format=vscode",
    "api:json": "wca analyze 'projects/lithium/**/*.{js,ts}' --outDir=projects/docs/api --format=json",
    "api:md": "wca analyze 'projects/lithium/**/*.{js,ts}' --format=md"
  },
  "private": true,
  "dependencies": {
    "@a11y/focus-trap": "^1.0.2",
    "@angular/animations": "~9.0.0-rc.0",
    "@angular/common": "~9.0.0-rc.0",
    "@angular/compiler": "~9.0.0-rc.0",
    "@angular/core": "~9.0.0-rc.0",
    "@angular/forms": "~9.0.0-rc.0",
    "@angular/platform-browser": "~9.0.0-rc.0",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.0",
    "@angular/router": "~9.0.0-rc.0",
    "@types/prismjs": "^1.16.0",
    "blueprint-css": "^3.1.1",
    "date-fns": "^2.2.1",
    "lit-element": "^2.2.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.16.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.0",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.0",
    "@angular/cli": "~9.0.0-rc.0",
    "@angular/compiler-cli": "~9.0.0-rc.0",
    "@angular/language-service": "~9.0.0-rc.0",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "bundlesize": "^0.18.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0-rc.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "protractor": "~5.4.2",
    "rimraf": "^3.0.0",
    "sass": "^1.23.1",
    "source-map-explorer": "^2.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-no-inherit": "^1.1.8",
    "typescript": "~3.6.4",
    "wc-sass-render": "^1.2.1",
    "web-component-analyzer": "^0.1.18",
    "webpack-bundle-analyzer": "^3.5.2"
  }
}
